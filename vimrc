execute pathogen#infect()
filetype plugin indent on

set encoding=utf-8

syntax on
set nu
set expandtab
set tabstop=4
set shiftwidth=4

map <F2> :NERDTreeToggle<CR>
map <F8> :TagbarToggle<CR>

set pastetoggle=<F3>
let g:airline#extensions#tabline#enabled = 1
let g:airline_powerline_fonts=1

:color cobalt
